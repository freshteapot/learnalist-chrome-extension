{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "cookies",
    "activeTab",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Learnalist",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "manifest_version": 2
}
