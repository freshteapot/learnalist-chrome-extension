{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "clipboardRead",
    "https://learnalist.net/*",
    "http://localhost/*",
    "http://localhost:1234/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://localhost:1234/*"
      ],
      "js": ["hello.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Learnalist",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "manifest_version": 2
}
